<?xml version="1.0" encoding="utf-8"?>
<Flowchart xmlns="http://schemas.microsoft.com/netfx/2009/xaml/activities" xmlns:tab="clr-namespace:TestCases.Activitiess.Bpm;assembly=TestCases.Workflows" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Flowchart.StartNode>
    <FlowSplit>
      <FlowSplit.Branches>
        <FlowSplitBranch>
          <FlowSplitBranch.StartNode>
            <FlowStep>
              <tab:AddStringActivity DisplayName="branch1" Item="branch1" />
