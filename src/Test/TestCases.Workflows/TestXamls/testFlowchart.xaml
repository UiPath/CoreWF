                  </FlowMergeAll.Next>
                </FlowMergeAll>
              </FlowStep.Next>
            </FlowStep>
          </FlowSplitBranch.StartNode>
        </FlowSplitBranch>
        <FlowSplitBranch>
          <FlowSplitBranch.StartNode>
            <FlowStep>
              <tab:AddStringActivity DisplayName="branch2" Item="branch2" />
              <FlowStep.Next>
                <x:Reference>__ReferenceID0</x:Reference>
              </FlowStep.Next>
            </FlowStep>
          </FlowSplitBranch.StartNode>
        </FlowSplitBranch>
      </FlowSplit.Branches>
    </FlowSplit>
  </Flowchart.StartNode>
